<?XML VERSION="1.0" ENCODING="UTF-8" ?>
<MediaGraph>
	<Log>
		<!--控制台输出,1为true-->
		<Consolelog value="1" />
		<!--系统输出,1为true-->
		<Systemlog value="0" />
		<!--文件输出,1为true-->
		<Filelog value="0" />
		<!--控制输出颜色，1为彩色-->
		<Color value="1" />
		<!-- 日志级别 error 输出错误  warn 输出警告  info 输出信息  debug 输出调试信息  trace 输出跟踪信息 -->
		<LogLevel value="debug" />
		<!--日志保存路径-->
		<LogSavePath value="/var/log" />
	</Log>
	
	<MediaElementParam type="1" owner="vCamCap">
		<FilePath value="Lenovo EasyCamera" />
		<VideoWidth value="640" />
		<VideoHeight value="480" />
		<ImgFormat value="1" />
		<VideoFPS value="30" />
	</MediaElementParam>
	
	<MediaElementParam type="2" owner="vScaler">
		<VideoWidth value="640" />
		<VideoHeight value="480" />
		<ImgFormat value="0" />
	</MediaElementParam>
	
	<MediaElementParam type="2" owner="vEnc">
		<VideoFPS value="30" />
		<Bitrate value="4096" />
		<BitrateRange min="0" max="0" />
		<QValue min="20" max="50" />
		<IFrameInterval value="20" />
		<CodecPreset value="fast" />
		<CodecControl value="cbr" />
		<CodecQuality value="35" />
		<CodecProfile value="baseline" />
	</MediaElementParam>
	
	<MediaElementParam type="1" owner="aMicCap">
		<FilePath value="外接麦克风 (Conexant SmartAudio HD)" />
		<SampleRate value="44100" />
		<AudioChannel value="2" />
		<SampleFormat value="1" />
	</MediaElementParam>
	
	<MediaElementParam type="2" owner="aAudioBuffer">
		<FrameSampleCount value="1024" />
	</MediaElementParam>
	
	<MediaElementParam type="3" owner="rtspSink">
		<InPortCount value="2" />
	</MediaElementParam>
	
	
	<PipelineSelector>
		<!-- 当前选用Pipeline组建模板，default 默认模板，一路音视频流输出， userdefine 自定义Pipeline模板-->
		<model value="default" index="0"/>
	</PipelineSelector>
</MediaGraph>